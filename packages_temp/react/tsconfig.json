{
    "compilerOptions": {
        "incremental": false,
        "target": "ES2020",
        "lib": [
            "ES2022",
            "DOM",
            "DOM.Iterable"
        ],
        "jsx": "react-jsx",
        "module": "Node16",
        "moduleResolution": "Node16",
        "allowJs": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "outDir": ".",
        "strict": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}